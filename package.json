{
  "name": "vue-convert-units",
  "description": "Convert units with vue integrating convert-units",
  "version": "0.1.0",
  "private": false,
  "author": "codekraft-studio",
  "main": "./dist/build.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name build ./src/ConvertUnits.js",
    "build-demo": "vue-cli-service build --dest docs",
    "publish-demo": "git subtree push --prefix docs origin gh-pages",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "convert-units": "^2.3.4",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.7",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.7",
    "@vue/cli-service": "^3.0.0-rc.7",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
